<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="view_res_partner_form_partenariat" model="ir.ui.view">
            <field name="name">res.partner.form.partenariat</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook" position="inside">
                    <page string="Partenariat" name="partenariat">
                        <group>
                            <field name="partenaire_type"/>
                            <field name="partenaire_secteur"/>
                            <field name="partenaire_engagement"/>
                            <field name="partenaire_date_renouvellement"/>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
